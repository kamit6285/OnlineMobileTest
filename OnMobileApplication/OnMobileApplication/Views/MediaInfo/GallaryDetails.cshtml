@model IEnumerable<OnMobileApplication.Models.ListSearchModel>

@{
    ViewBag.Title = "GallaryDetails";
}

<h2>GallaryDetails</h2>
<script src="~/Scripts/demo.js"></script>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GallaryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Count)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GallaryName)
        </td>
        <td>
            <button onclick="showDetail('@item.GallaryName')">@item.Count.ToString()</button>
            @*<a onclick="showDetail('@item.GallaryName')">@item.Count.ToString()</a>*@
           @* @Html.ActionLink(item.Count.ToString(),null,null,new { @onclick= "showDetail('" + item.GallaryName+"')"})*@
        </td>
    </tr>
}

</table>
<div id="detailShowId"></div>





